!include ../galera_2nodes.cnf

[mysqld]
wsrep_sst_method=xtrabackup-v2
wsrep_sst_auth="root:"

[xtrabackup]
close-files
# compression requires qpress from the Percona repositories
# compress
# compress-threads=2
encrypt=AES256
encrypt-key=4FA92C5873672E20FB163A0BCB2BB4A4
galera-info
history=backup
parallel=2

[SST]
encrypt=1
encrypt-algo=AES256
encrypt-key=4FA92C5873672E20FB163A0BCB2BB4A4
compressor="gzip"
decompressor="gzip -d"
